    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT c.CountryName, d.DirectorName, f.FilmName, f.FilmRunTimeMinutes
FROM     tblFilm AS f INNER JOIN
                  tblDirector AS d ON f.FilmDirectorID = d.DirectorID INNER JOIN
                  tblCountry AS c ON f.FilmCountryID = c.CountryID</CommandText>
      </Query>
      <Fields>
        <Field Name="CountryName">
          <DataField>CountryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
