  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT FilmNAME, tblDirector.DirectorName, tblFilm.FilmReleaseDate
FROM     tblDirector CROSS JOIN
                  tblFilm</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="FilmNAME">
          <DataField>FilmNAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DirectorName">
          <DataField>DirectorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmReleaseDate">
          <DataField>FilmReleaseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="FirstName">
          <Value>=LEFT(Fields!DirectorName.Value,len(Fields!DirectorName.Value) - Instr(Fields!DirectorName.Value, " "))</Value>
        </Field>
        <Field Name="LastName">
          <Value>=RIGHT(Fields!DirectorName.Value,len(Fields!DirectorName.Value) - Instr(Fields!DirectorName.Value, " "))</Value>
        </Field>
        <Field Name="MovieAge">
          <Value>=DateDiff(DateInterval.Year, Fields!FilmReleaseDate.Value,Today())</Value>
        </Field>
      </Fields>
    </DataSet>
