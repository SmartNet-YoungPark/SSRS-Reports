  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FilmName">
            <Value>=Parameters!FilmName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Minimum_RunTime">
            <Value>=Parameters!Minimum_RunTime.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DirectorName">
            <Value>=Parameters!DirectorName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT tblFilm.FilmName, tblFilm.FilmReleaseDate, tblFilm.FilmRunTimeMinutes, tblFilm.FilmDirectorID, tblDirector.DirectorName
FROM     tblFilm INNER JOIN
                  tblDirector ON tblFilm.FilmDirectorID = tblDirector.DirectorID
WHERE  (tblFilm.FilmName = @FilmName) AND (tblFilm.FilmReleaseDate &lt; @StartDate) AND (tblFilm.FilmRunTimeMinutes &gt; @Minimum_RunTime) OR
                  (tblFilm.FilmReleaseDate &lt; @StartDate) AND (tblFilm.FilmRunTimeMinutes &gt; @Minimum_RunTime) AND (@FilmName IS NULL) OR
                  (tblFilm.FilmName = @FilmName) AND (tblFilm.FilmRunTimeMinutes &gt; @Minimum_RunTime) AND (@StartDate IS NULL) OR
                  (tblFilm.FilmRunTimeMinutes &gt; @Minimum_RunTime) AND (@FilmName IS NULL) AND (@StartDate IS NULL) OR
                  (tblFilm.FilmName = @FilmName) AND (tblFilm.FilmReleaseDate &lt; @StartDate) AND (@Minimum_RunTime IS NULL) OR
                  (tblFilm.FilmReleaseDate &lt; @StartDate) AND (@FilmName IS NULL) AND (@Minimum_RunTime IS NULL) OR
                  (tblFilm.FilmName = @FilmName) AND (@StartDate IS NULL) AND (@Minimum_RunTime IS NULL) AND (@DirectorName = tblDirector.DirectorID OR
                  @DirectorName IS NULL) OR
                  (@FilmName IS NULL) AND (@StartDate IS NULL) AND (@Minimum_RunTime IS NULL) AND (@DirectorName = tblDirector.DirectorID OR
                  @DirectorName IS NULL)
ORDER BY tblDirector.DirectorName</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmReleaseDate">
          <DataField>FilmReleaseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="FilmRunTimeMinutes">
          <DataField>FilmRunTimeMinutes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FilmDirectorID">
          <DataField>FilmDirectorID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DirectorName">
          <DataField>DirectorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DirectorID, DirectorName, DirectorName as 'Sort Field'
FROM     tblDirector
UNION
SELECT NULL, 'Show ALL THE LIST','3'
ORDER BY 'Sort Field'</CommandText>
      </Query>
