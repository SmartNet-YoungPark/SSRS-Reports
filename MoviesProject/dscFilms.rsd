        <rd:TypeName>System.Int32</rd:TypeName>
      </Field>
      <Field Name="FilmDirectorID">
        <DataField>FilmDirectorID</DataField>
        <rd:TypeName>System.Int32</rd:TypeName>
      </Field>
      <Field Name="FilmLanguageID">
        <DataField>FilmLanguageID</DataField>
        <rd:TypeName>System.Int32</rd:TypeName>
      </Field>
      <Field Name="FilmCountryID">
        <DataField>FilmCountryID</DataField>
