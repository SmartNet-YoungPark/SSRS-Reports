      <Field Name="FilmBoxOfficeDollars">
        <DataField>FilmBoxOfficeDollars</DataField>
        <rd:TypeName>System.Int32</rd:TypeName>
      </Field>
      <Field Name="FilmOscarNominations">
        <DataField>FilmOscarNominations</DataField>
        <rd:TypeName>System.Int32</rd:TypeName>
